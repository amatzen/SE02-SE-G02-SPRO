                            <font>
                                <Font size="18.0"/>
                            </font>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </JFXButton>
                        <JFXButton fx:id="profileBtn" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                   minWidth="-Infinity" onAction="#handle" prefHeight="62.0" prefWidth="150.0"
                                   style="-fx-background-color: #FF9800;" text="Test"/>
                    </children>
                </HBox>
            </children>
        </HBox>
    </children>
