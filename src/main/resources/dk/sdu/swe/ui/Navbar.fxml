                                   prefWidth="200.0" text="Personer" textFill="WHITE">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </JFXButton>
                    </children>
                </HBox>
                <HBox alignment="TOP_RIGHT" prefHeight="62.0" prefWidth="440.0" HBox.hgrow="ALWAYS">
                    <children>
                        <JFXButton fx:id="adminBtn" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                                   minHeight="-Infinity" minWidth="-Infinity" onAction="#handle" prefHeight="62.0"
