<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
       <Label text="Review">
           <font>
               <Font name="System Bold" size="26.0" />
           </font>
           <VBox.margin>
               <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
           </VBox.margin>
       </Label>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
          <JFXListView fx:id="reviews" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      </AnchorPane>
   </children>
</fx:root>
