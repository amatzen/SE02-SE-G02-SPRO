                  </JFXTextField>
                  <JFXTextField fx:id="cvrNumber" layoutX="40.0" layoutY="110.0" prefHeight="49.0" prefWidth="309.0" promptText="CVR" style="-fx-background-color: transparent;                                 -fx-border-color: #CCCCCC;                                 -fx-padding: 0.333333em 1em 0.333333em 1em;                                 -jfx-unfocus-color: transparent;                                 -fx-border-radius: 5px; -jfx-focus-color: transparent;">
