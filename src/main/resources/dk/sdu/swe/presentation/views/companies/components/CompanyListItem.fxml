<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<fx:root maxWidth="1.7976931348623157E308" type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <HBox maxWidth="1.7976931348623157E308">
        <children>
            <VBox alignment="CENTER">
                <children>
                    <ImageView fitHeight="74.0" fitWidth="89.0" pickOnBounds="true" />
                </children>
                <HBox.margin>
                    <Insets left="20.0" />
                </HBox.margin>
            </VBox>
            <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
                <children>
                    <Label fx:id="companyNameLabel" alignment="BOTTOM_LEFT" prefHeight="45.0" prefWidth="143.0" text="Nordisk Film TV A/S">
                        <font>
                            <Font name="System Bold" size="14.0" />
                        </font>
                        <VBox.margin>
                            <Insets />
                        </VBox.margin>
                    </Label>
                    <Label fx:id="cvrLabel" alignment="TOP_LEFT" prefHeight="46.0" prefWidth="143.0" text="CVR " />
                </children>
                <HBox.margin>
                    <Insets left="20.0" />
                </HBox.margin>
            </VBox>
            <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
                <children>
                    <JFXButton fx:id="actionsBtn" alignment="CENTER" prefHeight="52.0" prefWidth="129.0" style="-fx-background-color: transparent;" text="Handlinger" textFill="#ff9800">
                        <font>
                            <Font name="System Bold" size="14.0" />
                        </font>
                        <VBox.margin>
                            <Insets left="50.0" />
                        </VBox.margin>
                    </JFXButton>
                </children>
                <HBox.margin>
                    <Insets left="100.0" />
                </HBox.margin>
            </VBox>
        </children>
    </HBox>
</fx:root>
